# LabVIEW project files & binaries
*.vi     filter=lfs diff=lfs merge=lfs -text
*.ctl    filter=lfs diff=lfs merge=lfs -text
*.llb    filter=lfs diff=lfs merge=lfs -text
*.lvproj filter=lfs diff=lfs merge=lfs -text
*.lvlps  filter=lfs diff=lfs merge=lfs -text
*.aliases filter=lfs diff=lfs merge=lfs -text
*.db     filter=lfs diff=lfs merge=lfs -text

# Images / docs (if present)
*.png    filter=lfs diff=lfs merge=lfs -text
*.jpg    filter=lfs diff=lfs merge=lfs -text
*.jpeg   filter=lfs diff=lfs merge=lfs -text
*.pdf    filter=lfs diff=lfs merge=lfs -text
